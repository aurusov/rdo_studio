			params[Expression::CONTEXT_PARAM_SET_OPERATION_TYPE] = setOperationType;
			Context::FindResult result = PARSER->context()->find(Context::METHOD_SET, params, pParamName->src_info());
			LPExpression pExpression = result.getCreateExpression()();
			pExpression->calc()->setSrcInfo(RDOParserSrcInfo(@1, @2, rdo::format("%s%s", pParamName->value().getIdentificator().c_str(), rdo::runtime::SetOperationType::toString(setOperationType).c_str())));
