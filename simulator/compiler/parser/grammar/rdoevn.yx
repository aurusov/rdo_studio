	{
		#PASS2
		{
			LPRDOValue pName = PARSER->stack().pop<RDOValue>($1);
			ASSERT(pName);
			bool traceValue = $3 != 0;

			Context::Params params;
			params[Context::Params::IDENTIFIER] = pName->value().getIdentificator();
			Context::FindResult result = PARSER->context()->find(Context::METHOD_OPERATOR_DOT, params, pName->src_info());
			ASSERT(result.getSwitchContext());

			Context::Params traceParams;
			traceParams["traceValue"] = traceValue;
			result = result.getSwitchContext().context->find("trace()", traceParams, pName->src_info());
			
			LPExpression pExpression = result.getCreateExpression()();
			ASSERT(pExpression);

			rdo::runtime::LPRDOCalc pCalc = pExpression->calc();
			LPRDORSSResource pResource = PARSER->findRSSResource(pName->value().getIdentificator());
			ASSERT(pResource);
			pResource->setTraceCalc(pCalc);
		}
	}
