$Resource_type  Бревно  : temporary
$Parameters
  номер                   : integer = 0
  длина                   : integer [0..2400]   /* в см */
  диаметр_н               : integer [0..720]    /* в мм */  /* Durchmesser */
  диаметр_к               : integer [0..720]    /* в мм */
  объем_кубический_м      : real
  нач_длина               : such_as Бревно.длина
  нач_объем_кубический_м  : such_as Бревно.объем_кубический_м
  вид_древесины           : (сосна, ель)
  качество                : integer [1..3]
  статус                  : (ничего, измерение, раскрой, раскроено,
                             готово_к_отпилке_торца, 
                             позиционируется, позиционировано, распиловка, готово)
  код_места               : (проходит,
                             Тр_1, Тр_2, Тр_3,
                             Кн_1, Кн_2, Кн_3, Кн_4, Кн_5,
                             Ск_1,  Ск_2,  Ск_3,  Ск_4,  Ск_5,  Ск_6,  Ск_7,  Ск_8,
                             Ск_9,  Ск_10, Ск_11, Ск_12, Ск_13, Ск_14, Ск_15, Ск_16,
                             Ск_17, Ск_18, Ск_19, Ск_20, Ск_21, Ск_22, Ск_23, Ск_24,
                             Ск_25, Ск_26, Ск_27, Ск_28, Ск_29, Ск_30)
  счетчик_оборотов        : integer
  позиция                 : integer  /* в см */
  использование_длины     : real
  использование_куб_м     : real
$End

$Resource_type  Бревно_показать : permanent
$Parameters
  номер                   : integer = 0
  длина                   : such_as Бревно.длина = 0
  код_места               : such_as Бревно.код_места = проходит
  счетчик_оборотов        : such_as Бревно.счетчик_оборотов = 0
  позиция                 : such_as Бревно.позиция = 0
$End

$Resource_type  Транспортер : permanent
$Parameters
  код                     : such_as Бревно.код_места
  скорость                : real
  текущее_кол_бревен      : integer
  длина                   : integer /* в см */
  счетчик                 : integer = 0
  статус                  : (вперед, остановлен) = остановлен
  движение                : (да, нет) = нет
  счетчик_оборотов        : integer = -32767
  позиция                 : integer /* в см */ = 0
  ген_длина               : integer = 0
  ген_диаметр             : integer = 0
  получить_вид_древесины  : such_as Бревно.вид_древесины = сосна
  получить_качество       : such_as Бревно.качество = 1
$End

$Resource_type  Конвейер : permanent
$Parameters
  код                     : such_as Бревно.код_места
  скорость                : real    /* в м/мин */
  шаг                     : integer = 25  /* в см, шаг 5 */
  текущее_кол_бревен      : integer
  длина                   : integer /* в см */
  статус                  : (вперед, назад, остановлен) = вперед
  движение                : (да, нет) = нет
  счетчик_оборотов        : integer = -32767
  позиция                 : integer /* в см */ = 0
$End

$Resource_type  Склад : permanent
$Parameters
  код                     : such_as Бревно.код_места
  расстояние              : integer
  сторона                 : (левая, правая)
  длина                   : real
  глубина                 : real = 2.0
  ширина                  : real = 2.0
  текущее_кол_бревен      : integer = 0
  номер_заказа            : integer = 0
  номер_позиции           : integer = 0
$End

$Resource_type  Позиция_измерения : permanent
$Parameters
  состояние                    : (свободен, занят) = свободен
  длина                   : integer = 0
  диаметр_н               : integer = 0
  диаметр_к               : integer = 0
  объем_кубический_м      : real = 0.0
$End

$Resource_type  Позиция_заказа : permanent
$Parameters
  номер_заказа            : integer
  номер_позиции           : integer
  имя_клиента             : (Бронштейн, Шниперсон, Крузенштерн, М_фон_Судов) = Шниперсон
  количество              : integer
  дата_обслуживания       : real
  время_прибытия          : real = 0.0
  типоразмер              : (  _60x60,   _60x80,  _60x120,  _80x80,   _80x100,
                              _80x120, _100x100, _100x120, _120x120, _120x140,
                             _120x160, _140x140, _140x160, _160x160, _160x180,
                             _100x200, _100x220, _120x200, _120x240, _160x200,
                             _180x220, _200x200, _200x240) = _60x60
  длина                   : integer [50..2400] /* в см */  /* Laenge */
  ширина                  : integer [50..300]  /* в мм */  /* Breite */
  высота                  : integer [10..300]  /* в мм */  /* Hoehe */
  объем                   : real               /* в куб м */
  вид_древесины           : such_as Бревно.вид_древесины
  класс                   : (S, A, B, C) = S
  качество                : such_as Бревно.качество
  приоритет               : integer [1..10] = 1
  кол_готовых             : integer = 0
  кол_пилоблоков          : integer = 0
  склад                   : such_as Бревно.код_места = проходит
  статус                  : (в_обработке, обработано) = обработано
$End

$Resource_type  Заказ_для_статистики : temporary
$Parameters
  номер_заказа            : integer
  номер_позиции           : integer
  дата_обслуживания       : real
  время_прибытия          : real
  количество              : integer
  кол_готовых             : integer
  кол_пилоблоков          : integer
  треб_диам_1             : real
  треб_диам_2             : real
  треб_диам_3             : real
  треб_диам_4             : real
  треб_диам_6             : real
  треб_диам_8             : real
  треб_диам_9             : real
$End

$Resource_type  Пилоблоки : temporary
$Parameters
  номер                   : integer = 0
  длина                   : such_as Бревно.длина
  диаметр_н               : such_as Бревно.диаметр_к
  диаметр_к               : such_as Бревно.диаметр_к
  объем_кубический_м      : such_as Бревно.объем_кубический_м
  вид_древесины           : such_as Бревно.вид_древесины
  качество                : such_as Бревно.качество
  код_места               : such_as Бревно.код_места
  позиция                 : such_as Бревно.позиция
  счетчик_оборотов        : such_as Бревно.счетчик_оборотов = 0
  номер_заказа            : such_as Позиция_заказа.номер_заказа
  номер_позиции           : such_as Позиция_заказа.номер_позиции
  код_склада              : such_as Бревно.код_места
  кол_пилоблоков          : integer [0..9]
  треб_диам               : real
  использование           : real
$End

$Resource_type  Распилочная_машина : permanent
$Parameters
  состояние               : (свободен, занят) = свободен
  длина                   : integer = 0
  диаметр_н               : integer = 0
  диаметр_к               : integer = 0
  объем_кубический_м      : real    = 0.0
  кол_пилоблоков          : integer [0..9] = 0
  тек_номер_пилоблоков    : integer [0..10] = 0
  длина_брутто            : integer = 0
  длина_нетто             : integer = 0
  объем_кубический_м_брутто  : real    = 0.0
  объем_кубический_м_нетто   : real    = 0.0
  длина_для_отпила        : integer = 0
  диаметр_н_для_отпила    : integer = 0
  счетчик_бревен          : integer = -3276
  бревна_обработаны       : integer = 0
  заказы_обработаны       : integer = 0
  обработано_пилоблоков   : integer = 0
  общая_длина_бревен      : real = 0.0  /* в метрах */
  общая_длина_остатка     : real = 0.0  /* в метрах */
  общий_объем_кубический_м_бревен   : real = 0.0
  общий_объем_кубический_м_остатка  : real = 0.0
  общий_объем_кубический_м_выхода   : real = 0.0
$End

$Resource_type  Результат_раскроя : permanent
$Parameters
  номер                   : integer [1..7]
  номер_заказа            : such_as Позиция_заказа.номер_заказа = 0
  номер_позиции           : such_as Позиция_заказа.номер_позиции = 0
  имя_клиента             : such_as Позиция_заказа.имя_клиента
  вид_древесины           : such_as Бревно.вид_древесины = сосна
  качество                : such_as Позиция_заказа.качество = 1
  класс                   : such_as Позиция_заказа.класс = S
  длина                   : such_as Позиция_заказа.длина = 100
  ширина                  : such_as Позиция_заказа.ширина = 100
  высота                  : such_as Позиция_заказа.высота = 100
  кол_пилоблоков          : such_as Пилоблоки.кол_пилоблоков = 0
  тек_диаметр             : such_as Бревно.диаметр_к = 200
  треб_диам               : real = 0.0
  объем_кубический_м      : such_as Бревно.объем_кубический_м = 0.0
  использование           : such_as Пилоблоки.использование = 0.0
  код_склада              : such_as Пилоблоки.код_склада = проходит
  termin                  : such_as Позиция_заказа.дата_обслуживания = 0.0
  soll                    : such_as Позиция_заказа.количество = 0
  lst                     : such_as Позиция_заказа.кол_готовых = 0
  накопл_длина            : such_as Бревно.длина = 0
$End

