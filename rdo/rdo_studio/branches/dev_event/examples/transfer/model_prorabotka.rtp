$Resource_type счетчики: permanent
$Parameters
    количество_обслуженных_заявок: integer = 0
	счет_заявок                  : integer = 0
	счет_формируемых_составов    : integer = 0
$End

$Resource_type заявки: temporary 
$Parameters
    номер_заявки               : integer = 0
	вес_груза		           : real    = 0.0
	количество_вагонов_в_заявке: integer = 0 
	маршрут                    : integer [1..3]
	состояние                  : (пришла, рассчитана, привязать_к_формируемому_составу, распределение_вагонов,
									остались_нераспределенные_вагоны, распределена, выполняется, обслужена)
	остаток_в_заявке           : integer = 0
	номер_формируемого_состава1: integer = 0 //цифра 1 - временное решение, потому что возможно будем 
$End										   //передавать в заявку еще один номер состава	

$Resource_type формируемые_составы: permanent 
$Parameters
    номер_формируемого_состава : integer = 1
	маршрут                    : such_as заявки.маршрут 
	текущее_количество_вагонов : integer = 0
	состояние                  : (привязать_к_следующей_заявке, распределение_вагонов, полупустой, создан, пустой) = пустой
	номер_распределяемой_заявки: integer = 0
	
$End

$Resource_type пункты_отправления : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: (Кульсары, Аксарайская, Красное, Брест, Малашевиче, Кошта, Гуково, Батево, Тужер) = Кульсары
$End

$Resource_type пункты_погрузки : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: such_as пункты_отправления.местоположение = Кульсары
$End

$Resource_type таможенные_пункты : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: such_as пункты_отправления.местоположение
$End

$Resource_type пункты_смены_колеи : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: such_as пункты_отправления.местоположение = Брест
$End

$Resource_type пункты_назначения : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: such_as пункты_отправления.местоположение
$End

$Resource_type пункты_мойки : permanent
$Parameters
	состояние   : (свободен, занят) = свободен
	местоположение: such_as пункты_отправления.местоположение
$End

$Resource_type составы : temporary
$Parameters
	состояние     : (не_существует, сформирован, создается, готов_к_погрузке, погрузка, готов_к_отправке, 
					 в_пути, прибыл_на_таможню, прохождение_таможни, необходимо_сменить_колею, смена_колеи, прибыл,
					 прибыл_на_разгрузку, разгрузка, необходима_мойка, мойка, готов_к_возврату,
					 прибыл_в_парк) = не_существует
	маршрут       : such_as заявки.маршрут
	местоположение: such_as пункты_отправления.местоположение = Кульсары
	номер_состава : integer = 0
	направление   : (прямое, обратное)
$End

$Resource_type парки : permanent
$Parameters
	количество_вагонов_в_парке: integer [0..1000] = 1000
	дефицит                   : integer = 0 
$End
