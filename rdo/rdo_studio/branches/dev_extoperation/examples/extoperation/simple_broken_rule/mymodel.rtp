$Resource_type system_type : permanent
$Parameters
	sys_time  : real    = 0
	part_done_count: integer = 0
	part_broken_count: integer = 0
	opr_count : integer = 0
$End

$Resource_type machine_tool_type : permanent
$Parameters
	opr_count: integer = 0
	a        : integer = 0
	status   : (free, busy, broken) = free
$End

$Resource_type part_type : permanent
$Parameters
	status    : (free, busy, ready, broken) = free
	opr_count : integer = 0
	begin_time: real    = 0
	opr_time  : real    = 0
$End
