$Process
//Появление заготовок в системе каждые 150 секунд по экспоненциальному закону
	GENERATE 21 + 1//Интервал_прихода_заготовок(200) 

//Перемещение заготовки из зоны приемки заготовок к первому станку
	QUEUE	 QROBOT1
	SEIZES	 ROBOT
//	ASSIGNE  ROBOT.Состояние_для_анимации = несет_к_станку_1
	DEPART	 QROBOT1
	ADVANCE  Время_захвата(8,1)
	ADVANCE  10
	ADVANCE  Время_отпуска(8,1)
	RELEASES ROBOT
//	RELEASE  ROBOT 
//	ASSIGNE  ROBOT.Состояние_для_анимации = деталь_на_станке_1

//Обработка на первом станке
	QUEUE	 QSTANOK1
	SEIZES	 ST11, ST12, ST13 
	DEPART	 QSTANOK1
	ADVANCE  Обработка_на_станке_1(80,10)
	RELEASES ST11, ST12, ST13 
	
//Перемещение заготовки от перового станка ко второму

	QUEUE	 QROBOT2
	SEIZE	 ROBOT
	DEPART	 QROBOT2
	ADVANCE  Время_захвата(8,1)
	ADVANCE  7
	ADVANCE  Время_отпуска(8,1)
	RELEASE  ROBOT 
	
//Обработка на втором станке
	QUEUE	 QSTANOK2
	SEIZE	 STANOK2
	DEPART	 QSTANOK2
	ADVANCE  Обработка_на_станке_2(20)
	RELEASE  STANOK2 

//Перемещение заготовки от второго станка в зону готовых деталей
	QUEUE	 QROBOT3
	SEIZE	 ROBOT
	DEPART	 QROBOT3
	ADVANCE  Время_захвата(8,1)
	ADVANCE  15
	ADVANCE  Время_отпуска(8,1)
	RELEASE  ROBOT 
	
	QUEUE	 QEXIT
	
	TERMINATE 1 
$End
